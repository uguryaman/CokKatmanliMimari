        <div class="content-wrapper">
      
            <!-- Main content -->
            <section class="content">
                @RenderBody()
            </section>
            <!-- /.content -->
        </div>

    </div>
    <!-- ./wrapper -->
    <!-- jQuery -->
    <script src="~/Tema/Tema1/plugins/jquery/jquery.min.js"></script>
    <!-- jQuery UI 1.11.4 -->
    <script src="~/Tema/Tema1/plugins/jquery-ui/jquery-ui.min.js"></script>
    <!-- Resolve conflict in jQuery UI tooltip with Bootstrap tooltip -->
    <script>$.widget.bridge('uibutton', $.ui.button)</script>
    <!-- Bootstrap 4 -->
    <script src="~/Tema/Tema1/plugins/bootstrap/js/bootstrap.bundle.min.js"></script>
    <!-- ChartJS -->
    <script src="~/Tema/Tema1/plugins/chart.js/Chart.min.js"></script>
    <!-- Sparkline -->
    <script src="~/Tema/Tema1/plugins/sparklines/sparkline.js"></script>
    <!-- JQVMap -->
    <script src="~/Tema/Tema1/plugins/jqvmap/jquery.vmap.min.js"></script>
    <script src="~/Tema/Tema1/plugins/jqvmap/maps/jquery.vmap.usa.js"></script>
    <!-- jQuery Knob Chart -->
    <script src=~/Tema/Tema1/"plugins/jquery-knob/jquery.knob.min.js"></script>
    <!-- daterangepicker -->
    <script src="~/Tema/Tema1/plugins/moment/moment.min.js"></script>
    <script src="~/Tema/Tema1/plugins/daterangepicker/daterangepicker.js"></script>
    <!-- Tempusdominus Bootstrap 4 -->
    <script src="~/Tema/Tema1/plugins/tempusdominus-bootstrap-4/js/tempusdominus-bootstrap-4.min.js"></script>
